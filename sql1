--задание4
select distinct T.Tname
from T, STD, D, S
where T.Tnum = STD.Tnum 
and D.Dnum = STD.Dnum 
and S.Snum = STD.Snum 
and S.City = D.City;
  
--задание5
select distinct S.Sname
from S, STD
where S.Snum = STD.Snum 
and STD.Dnum = 'D1';
  
 --задание6
select distinct S.City AS SupplierCity, D.City AS OrganizationCity
from S, D, STD
where S.Snum = STD.Snum 
and D.Dnum = STD.Dnum;
  
--задание7
select distinct T.Tnum
from T, STD, S
where T.Tnum = STD.Tnum 
and S.Snum = STD.Snum 
and S.Status > 50;
